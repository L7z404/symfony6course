{% extends 'base.html.twig' %}

{% block body %}

    <h1>Default variables</h1>
    <p><b>_self:</b> {{_self}}</p>
    <p><b>_charset:</b> {{_charset}}</p>
    <p><b>author (specified in twig.yaml):</b> {{author}}</p>

    <h2>Movies showcasing tonight</h2>
    {% for movie in movies %}
        <li>{{movie}}</li>
    {% endfor %}

{% endblock %}